<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dongdada.openclaw-updater</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>${HOME}/workspace/openclaw-safe-updater/openclaw-updater.sh</string>
    </array>
    <key>EnvironmentVariables</key>
    <dict>
        <key>LOG_DIR</key>
        <string>${HOME}/workspace/logs</string>
    </dict>
    <key>StartCalendarInterval</key>
    <array>
        <dict>
            <key>Weekday</key>
            <integer>0</integer>
            <key>Hour</key>
            <integer>9</integer>
            <key>Minute</key>
            <integer>0</integer>
        </dict>
    </array>
    <key>RunAtLoad</key>
    <false/>
    <key>StandardOutPath</key>
    <string>${HOME}/workspace/logs/openclaw-updater.log</string>
    <key>StandardErrorPath</key>
    <string>${HOME}/workspace/logs/openclaw-updater.error.log</string>
</dict>
</plist>
